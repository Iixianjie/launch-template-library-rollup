    "autoprefixer": "^9.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "chokidar": "^3.0.1",
    "cssnano": "^4.1.10",
    "glob": "^7.1.4",
    "node-fs-extra": "^0.8.2",
    "node-sass": "^4.12.0",
    "react-dom": "^16.8.6",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.0.1",
