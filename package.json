{
  "name": "rollop-template",
  "version": "0.0.1",
  "description": "rollup library config",
  "main": "lib/index/index.js",
  "module": "esm/index/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "dev": "rollup -c build/rollup.config.dev.js --watch --environment BUILD:development",
